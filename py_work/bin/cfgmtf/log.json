{
    "version": 1,
    "formatters": {
        "json_format": {
	    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
 	}
    },
    "handlers": {
        "rotatefile": {
	    "class": "logging.handlers.RotatingFileHandler",
	    "filename": "./debug/log/mtf.log",
	    "level": "DEBUG",
	    "formatter": "json_format",
        "maxBytes": 10485760,
        "backupCount": 10
	}
    },
    "loggers":{
        "RotateFileLogger": {
	    "handlers": ["rotatefile"],
	    "level": "DEBUG"
	}
    }
}
